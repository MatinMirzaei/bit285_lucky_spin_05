@model IEnumerable<LuckySpin.Models.Spin>
@{
    Layout = "_Layout";
}

<h1>All Your Luck</h1>
<table>
    <thead>
        <tr>
            <th>Luck</th>
            <th>Result</th>
        </tr>
    </thead>
    <tbody>
        @foreach (LuckySpin.Models.Spin s in Model)
        {
        <tr>
            <th>@s.Luck</th>
            <th>@s.IsWinner</th>
        </tr>
        }
    </tbody>
</table>